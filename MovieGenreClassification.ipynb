    {
      "cell_type": "code",
      "source": [
        "# Convert plots to strings and handle missing values\n",
        "X_train = train_data['plot'].astype(str).fillna(\"\")  # Ensure all plots are strings\n",
        "y_train = train_data['genre']  # Genres\n"
